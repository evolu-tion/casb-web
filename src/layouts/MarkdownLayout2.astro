---
import Layout from '~/layouts/PageLayout.astro';
import type { MetaData } from '~/types';

export interface Props {
  frontmatter: {
    title?: string
  }
}

const { frontmatter } = Astro.props;

const metadata: MetaData = {
  title: frontmatter?.title,
};
---

<Layout metadata={metadata}>
  <slot />
<Layout metadata={metadata}>
